import "platform:/resource/Voter/model/Voter.ecore"

pattern disagree() {
	v1:Voter1 {
		-link-> v2
	}
	v2:Voter1 {
		-link-> v1
	}
	# v1.vote != v2.vote
}

pattern agree() {
	v1:Voter1 {
		-link-> v2
	}
	v2:Voter1 {
		-link-> v1
	}
	# v1.vote == v2.vote
}

rule newVoter(){
	++ v: Voter1 {
		++ -link-> v0
		++ -link-> v1
		++ -link-> v2
	}
	v0:Voter1 {
		++ -link-> v
	}
	v1:Voter1 {
		++ -link-> v
	}
	v2:Voter1 {
		++ -link-> v
	}
}
